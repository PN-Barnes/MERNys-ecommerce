    "type": "git",
    "url": "git+https://github.com/PN-Barnes/MERNys-ecommerce.git"
  },
  "author": "Paul Barnes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PN-Barnes/MERNys-ecommerce/issues"
  },
  "homepage": "https://github.com/PN-Barnes/MERNys-ecommerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mongoose": "^6.2.7"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
